  "author": "codetheflow",
  "bugs": {
    "url": "https://github.com/codetheflow/skintest/issues"
  },
  "homepage": "https://github.com/codetheflow/skintest#readme",
  "devDependencies": {
    "@types/node": "^15.0.1",
    "@typescript-eslint/eslint-plugin": "4.22.0",
    "@typescript-eslint/parser": "4.22.0",
    "cpx": "^1.5.0",
    "eslint": "7.25.0",
